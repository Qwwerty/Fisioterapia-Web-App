se resolve(false);\n        })\n        .catch(function(error) {\n          console.log(error);\n          resolve(false)\n          reject(false);\n        });\n    });\n  }\n\n  pedirSenha(email){\n    return new Promise((resolve, reject) => {\n      axios\n      .post('http://paralisiabackend-com-br.umbler.net/pedirSenha', {\n        email: email\n      })\n       /* eslint-disable */ \n      .then(function(response) {\n        resolve(true);\n      })\n      .catch(function(error) {\n        console.log(error);\n        reject(false)\n      });\n    })\n  }\n}\n"]}]}